package oopconcepts;

public class DemoMrthod {

	public static void main(String[] args) {
		anotherMethod("Shreeyana", 90);
		anotherMethod("RAm", 5);
		anotherMethod("Sita",50);
		anotherMethod("Hari", 100);
		anotherMethod("Krishna", 55);
		anotherMethod("Gita", 15);
		
	}
	public static void anotherMethod(String stuname ,int score) {
	
		 String grade;
		if (score>=90 && score<= 100) {
			grade="A";}
			else if (score > 70 && score<85) {
			grade="B";}
			else if(score>=50 && score<60) {
				grade="C";}
				else {
					grade="FAIL";}
		//" is: " lirako xaenaw
		System.out.println("Grade Of "+ stuname  +  grade);
		
		
	}
		}

	


